<h1>Example#show</h1>
<table>
  <tr>
    <th>Число</th>
    <th>Квадрат числа</th>
  </tr>
  <% @arr.length().times do |i| %>
    <tr>
      <td><%= @arr[i]%></td>
      <td><%= @arr[i]*@arr[i]%></td>
    </tr>
  <% end %>
</table>
<h3>Количество: <%= @arr.length() %> </h3>

<%= link_to 'Вернуться на страницу ввода', root_path %>
